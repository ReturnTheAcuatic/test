{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "baseUrl": "./",
    "paths": {
      "@root/*": ["./*"],
      "@handlers/*": ["./src/handlers/*"],
      "@utils/*": ["./src/utils/*"],
      "@models/*": ["./src/models/*"],
      "@src/*": ["./src/*"],
      "@structures/*": ["./src/structures/*"]
    }
  },
  "exclude": ["node_modules", "**/node_modules/*"]
}
